<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AgenciesIndex" table="agencies_index">
    <id name="agencyId" type="smallint" column="agency_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="agencyName" type="string" column="agency_name" length="120" nullable="true"/>
    <field name="agencyAddress" type="string" column="agency_address" length="120" nullable="true"/>
    <field name="agencyOffice" type="string" column="agency_office" length="50" nullable="true"/>
    <field name="agencyUri" type="string" column="agency_uri" length="255" nullable="true"/>
    <field name="createDate" type="integer" column="create_date" nullable="false"/>
    <field name="lat" type="string" column="lat" length="20" nullable="true"/>
    <field name="lng" type="string" column="lng" length="20" nullable="true"/>
    <field name="agencyEmail" type="string" column="agency_email" length="50" nullable="true"/>
    <field name="agencySite" type="string" column="agency_site" length="255" nullable="true"/>
    <field name="agencyUrl" type="text" column="agency_url" nullable="true"/>
  </entity>
</doctrine-mapping>
