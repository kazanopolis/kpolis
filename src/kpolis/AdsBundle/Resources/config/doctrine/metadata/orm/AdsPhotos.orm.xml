<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdsPhotos" table="ads_photos">
    <id name="photoId" type="integer" column="photo_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adId" type="integer" column="ad_id" nullable="false"/>
    <field name="filePath" type="string" column="file_path" length="255" nullable="true"/>
    <field name="createDate" type="integer" column="create_date" nullable="false"/>
    <field name="flStatus" type="boolean" column="fl_status" nullable="false"/>
    <field name="photoType" type="boolean" column="photo_type" nullable="false"/>
    <field name="flBad" type="boolean" column="fl_bad" nullable="false"/>
    <field name="width" type="smallint" column="width" nullable="false"/>
    <field name="height" type="smallint" column="height" nullable="false"/>
    <field name="size" type="integer" column="size" nullable="false"/>
  </entity>
</doctrine-mapping>
