<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContentSections" table="content_sections">
    <id name="sectionId" type="boolean" column="section_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sectionName" type="string" column="section_name" length="50" nullable="true"/>
    <field name="userId" type="integer" column="user_id" nullable="false"/>
    <field name="flHidden" type="boolean" column="fl_hidden" nullable="false"/>
    <field name="flShared" type="boolean" column="fl_shared" nullable="false"/>
  </entity>
</doctrine-mapping>
