<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TmpHousesIndex" table="tmp_houses_index">
    <id name="houseId" type="integer" column="house_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="houseDistrict" type="boolean" column="house_district" nullable="false"/>
    <field name="houseStreetId" type="smallint" column="house_street_id" nullable="false"/>
    <field name="houseNumber" type="smallint" column="house_number" nullable="false"/>
    <field name="houseDrob" type="smallint" column="house_drob" nullable="false"/>
    <field name="houseBlock" type="string" column="house_block" length="4" nullable="true"/>
    <field name="houseKorpus" type="string" column="house_korpus" length="6" nullable="true"/>
    <field name="houseFloors" type="boolean" column="house_floors" nullable="false"/>
    <field name="houseFloors2gis" type="boolean" column="house_floors_2gis" nullable="false"/>
    <field name="houseStatus" type="boolean" column="house_status" nullable="false"/>
    <field name="lat" type="string" column="lat" length="20" nullable="true"/>
    <field name="lng" type="string" column="lng" length="20" nullable="true"/>
    <field name="flManual" type="boolean" column="fl_manual" nullable="false"/>
    <field name="houseZhk" type="boolean" column="house_zhk" nullable="false"/>
    <field name="houseCrossStreet" type="string" column="house_cross_street" length="100" nullable="false"/>
    <field name="houseCrossNumber" type="string" column="house_cross_number" length="10" nullable="true"/>
    <field name="housePurpose" type="string" column="house_purpose" length="100" nullable="true"/>
    <field name="houseName" type="string" column="house_name" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
